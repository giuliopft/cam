    "license": "MIT",
    "repository": "github:giuliopft/cam",
    "version": "1.0.0",
    "description": "A Cards Against Humanity clone.",
    "main": "build/app.js",
    "private": true,
    "scripts": {
        "start": "node build/app.js",
        "build-backend": "tsc --project ./",
        "wd": "webpack --config webpack.dev.js",
        "wp": "webpack --config webpack.prod.js",
        "buildd": "rm -r ./build && npm run build-backend && npm run wd",
        "buildp": "rm -r ./build && npm run build-backend && npm run wp",
        "alld": "npm run buildd && npm run start",
        "allp": "npm run buildp && npm run start"
    },
    "dependencies": {
        "@jsdsl/bimap": "^0.1.1",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.9",
        "@types/node": "^14.14.19",
        "css-loader": "^5.0.1",
        "file-loader": "^6.2.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.1",
        "mini-css-extract-plugin": "^1.3.3",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.0",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "vue": "^2.6.12",
        "vue-loader": "^15.9.6",
        "vue-template-compiler": "^2.6.12",
        "webpack": "^5.11.1",
        "webpack-cli": "^4.3.1",
        "webpack-merge": "^5.7.3"
    }
}
